<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="user_doctor_house" model="res.users">
            <field name="name">Dr. House (User)</field>
            <field name="login">house</field>
            <field name="password">house</field>
            <field name="groups_id" eval="[(4, ref('base.group_user'))]"/>
        </record>
        <record id="user_doctor_intern" model="res.users">
            <field name="name">Dr. Intern (User)</field>
            <field name="login">intern</field>
            <field name="password">intern</field>
            <field name="groups_id" eval="[(4, ref('base.group_user'))]"/>
        </record>
        <record id="doctor_house" model="hr.hospital.doctor">
            <field name="last_name">Хаус</field>
            <field name="first_name">Грегорі</field>
            <field name="specialty_id" ref="spec_therapist"/>
            <field name="license_number">LIC-HOUSE</field>
            <field name="license_date">2005-05-10</field>
            <field name="rating">4.9</field>
            <field name="is_intern">False</field>
            <field name="user_id" ref="user_doctor_house"/>
        </record>
        <record id="doctor_strange" model="hr.hospital.doctor">
            <field name="last_name">Стрендж</field>
            <field name="first_name">Стівен</field>
            <field name="specialty_id" ref="spec_surgery"/>
            <field name="license_number">LIC-STRANGE</field>
            <field name="license_date">2010-01-15</field>
            <field name="rating">4.7</field>
            <field name="is_intern">False</field>
        </record>
        <record id="doctor_who" model="hr.hospital.doctor">
