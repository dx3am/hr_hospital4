            <field name="specialty_id" ref="spec_therapist"/>
            <field name="license_number">LIC-DORY</field>
            <field name="license_date" eval="(DateTime.now() - relativedelta(months=6)).strftime('%Y-%m-%d')"/>
            <field name="rating">3.0</field>
            <field name="is_intern">True</field>
            <field name="mentor_id" ref="doctor_house"/>
            <field name="user_id" ref="user_doctor_intern"/>
        </record>
        <record id="intern_turk" model="hr.hospital.doctor">
